<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:controller="fx.excel.upload.controller.ExcelListController" xmlns:fx="http://javafx.com/fxml" >

	<AnchorPane id="list">
		<children>
		    <ListView fx:id="excelListView" styleClass="excel-list" />
		 </children>
	</AnchorPane>

	<HBox>
		<Button fx:id="fileListRefreshBtn" onAction="#handleListRefresh" text="再読込"/>
		<Button fx:id="fileDetailShowBtn" text="閲覧"/>
	</HBox>

	<AnchorPane id="detail">
		<children>
			<Label fx:id="excelDetail" text="upload-detail" />
		</children>
	</AnchorPane>
</VBox>